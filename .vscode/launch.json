{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/voiceflow-pro/apps/api/src/services/transcription.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
        ,
        {
            "type": "node",
            "request": "launch",
            "name": "Debug API (tsx)",
            "runtimeExecutable": "npx",
            "runtimeArgs": ["tsx", "src/server.ts"],
            "cwd": "${workspaceFolder}/voiceflow-pro/apps/api",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": ["<node_internals>/**"],
            "envFile": "${workspaceFolder}/voiceflow-pro/apps/api/.env"
        }
    ]
}